{
  "name": "@cramkle/cramkle",
  "version": "0.18.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "release-it",
    "dev": "concurrently \"NODE_ENV=development casterly watch\" \"NODE_ENV=development nodemon server.js -i build/\"",
    "start": "NODE_ENV=production node server.js",
    "build": "casterly build",
    "prebuild": "yarn compile --strict",
    "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'",
    "test": "NODE_ENV=test jest",
    "lint": "eslint --ext js,ts,tsx .",
    "lint:css": "stylelint '**/*.{scss,css}'",
    "lint:ts": "tsc --noEmit --pretty",
    "download-schema": "apollo service:download --endpoint=http://localhost:5000/_c/graphql -k",
    "gen-types": "apollo client:codegen __generated__ --target=typescript --localSchemaFile=schema.json --globalTypesFile=src/globalTypes.ts",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract --clean",
    "compile": "lingui compile --typescript"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{json,graphql,gql,scss,css}": [
      "prettier --write"
    ],
    "*.{scss,css}": [
      "stylelint"
    ]
  },
