{
  "name": "@cramkle/cramkle",
  "version": "0.10.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "release-it",
    "dev": "rs dev",
    "start": "rs start",
    "build": "rs build",
    "build-tailwind": "tailwindcss build src/tailwind.css -o src/_tailwind.global.css",
    "prebuild": "yarn compile --strict",
    "prepare": "yarn build-tailwind",
    "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'",
    "test": "NODE_ENV=test jest",
    "lint": "eslint --ext ts,tsx .",
    "lint:css": "stylelint '**/*.{scss,css}'",
    "lint:ts": "tsc --noEmit --pretty",
    "download-schema": "apollo service:download --endpoint=http://localhost:5000/graphql -k",
    "gen-types": "apollo client:codegen __generated__ --target=typescript --localSchemaFile=schema.json --globalTypesFile=src/globalTypes.ts",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract --clean",
    "compile": "lingui compile --typescript"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{json,graphql,gql}": [
      "prettier --write"
    ],
    "*.{scss,css}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@lingui/react": "3.0.0-13",
    "@material/icon-button": "^3.1.0",
    "@material/react-linear-progress": "^0.14.1",
    "@material/snackbar": "^3.1.0",
    "@reach/alert-dialog": "^0.11.2",
    "@reach/checkbox": "^0.11.2",
    "@reach/dialog": "^0.11.1",
    "@reach/listbox": "^0.11.2",
    "@reach/menu-button": "^0.11.2",
    "@reach/tabs": "^0.11.2",
    "@reach/tooltip": "^0.11.2",
    "@sentry/react": "^5.23.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.3",
    "classnames": "^2.2.6",
    "d3-array": "^2.7.1",
    "d3-scale": "^3.2.2",
    "d3-shape": "^1.3.7",
    "date-fns": "^2.16.1",
    "draft-js": "^0.11.7",
    "formik": "^2.1.5",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "history": "^4.7.2",
    "keycode-js": "^3.1.0",
    "make-plural": "^6.2.2",
    "node-fetch": "^2.6.1",
    "react": "^16.12.0",
    "react-app-server": "^0.1.9",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.12.19",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-tab-controller": "^0.2.1",
    "react-window": "^1.8.5",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^1.8.6",
    "universal-cookie": "^4.0.3",
    "uuid": "^8.3.0",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-precaching": "^5.1.2",
    "workbox-routing": "^5.1.2",
    "workbox-strategies": "^5.1.4",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.4",
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@lingui/cli": "3.0.0-13",
    "@lingui/macro": "3.0.0-13",
    "@lucasecdb/eslint-config": "^0.12.0",
    "@release-it/keep-a-changelog": "^2.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@types/classnames": "^2.2.9",
    "@types/d3-array": "^2.0.0",
    "@types/d3-scale": "^2.2.0",
    "@types/d3-shape": "^1.3.2",
    "@types/draft-js": "^0.10.44",
    "@types/exenv": "^1.2.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.13",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-window": "^1.8.2",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.6",
    "apollo": "^2.30.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-preset-react-app": "^9.1.1",
    "eslint": "^7.8.1",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^3.0.1",
    "jest-resolve": "^26.4.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "react-app-polyfill": "^1.0.2",
    "release-it": "^14.0.2",
    "stylelint": "^13.7.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^3.9.7"
  },
  "resolutions": {
    "graphql": "15.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": {
    "path": "/_c",
    "config": {
      "target": "http://localhost:5000",
      "pathRewrite": {
        "^/_c": ""
      }
    }
  }
}
